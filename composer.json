{
  "name": "rehfeld/comparator",
  "description": "Helps you write concise array sorting callbacks for php's usort function",
  "version": "0.3.0",
  "type": "library",
  "keywords": ["sorting", "usort", "comparator", "callback", "comparison"],
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Rehfeld",
      "email": "rehfeldchris@gmail.com",
      "homepage": "http://www.rehfeld.us"
    }
  ],
  "support": {
    "issues": "https://github.com/rehfeldchris/comparator/issues",
    "source": "https://github.com/rehfeldchris/comparator"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*"
  },
  "autoload": {
    "psr-0": {
      "Rehfeld\\Comparator"          : "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Rehfeld\\Comparator\\Test"  : "test/"
    }
  },
  "require": {
    "php": ">=5.3.0"
  }
}